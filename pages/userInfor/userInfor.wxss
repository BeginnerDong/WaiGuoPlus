/**index.wxss**/
@import "../../wxParse/wxParse.wxss"
page{
	
}
.user_img{
	width:150rpx;
	height:150rpx;
	border-radius:50%;
	overflow:hidden;
	border:solid 6rpx #fff;
	margin:0 auto;
}
.logo{
	width:100%;
	height:100vh;
}
.logo_top{
	width:100%;
	height:23%;
	margin-top:10rpx;
}
.logo_top image{
	width:200rpx;
	height:200rpx;
	margin:0 auto;
}
.input_list1{
	width:100%;
	height:100rpx;
	border-bottom:solid 1px #e0e0e0;
}
.input_list1 .input_before{
	width:22%;
}
.input_list1 input{
	width:76%;
	padding-left:2%;
	height:100%;
	border-radius:16rpx;
}
.input_list1 .input{
	width:76%;
	padding-left:2%;
	height:80rpx;
	line-height:80rpx;
	border-radius:16rpx;
	color:#222;
}
.input_list1 input{
	color:#222;
}
.placeholder{
	color:#ccc;
}
.data_item{
	width:29.33%;
	padding:0 2%;
	border-right:solid 1px #ccc;
	height:60rpx;
}
.input_list1 textarea{
	height:40rpx;
	padding-top:16rpx;
	line-height:normal;
}
.submit_info{
	width:670rpx;
	margin:40rpx auto;
	border-radius:10rpx;
}